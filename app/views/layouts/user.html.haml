!!!
%html
  %head
    %title Myclass103
    = stylesheet_link_tag    'application', media: 'all'
    = javascript_include_tag 'application'
    = csrf_meta_tags
  %body#body1
    %nav.navbar.navbar-default#navbartransparent
      .container-fluid
        .navbar-header
          %button.navbar-toggle.collapsed{"aria-controls" => "navbar", "aria-expanded" => "false", "data-target" => "#navbar", "data-toggle" => "collapse", :type => "button"}
            %span.sr-only Toggle navigation
            %span.icon-bar
            %span.icon-bar
            %span.icon-bar
          %a.navbar-brand.glyphicon.glyphicon-home{:href => "/"}
        #navbar.navbar-collapse.collapse
          %ul.nav.navbar-nav
            %li= link_to '#Myclass103', user_root_path, class: 'navbar-brand'
            %li#uno= link_to '#Dashboard',  user_root_path
            %li#uno= link_to '#My profile',  user_user_path(current_human)
            %li#uno= link_to '#Event groups',  user_event_groups_path
            %li#uno= link_to 'PoS (not)',  root_path
            %li#uno= link_to 'Payments (not)',  root_path
            %li#uno= link_to 'root',  root_path
            %li#uno= link_to 'Users',  user_users_path
            %li#uno= link_to 'Clients',  user_guests_path
          %ul.nav.navbar-nav.pull-right
            - if user_signed_in?
              %li.dropdown
                %a.dropdown-toggle#whitelink{"data-toggle" => "dropdown", :href => "#"}
                  = current_user.username
                  %span.caret
                %ul.dropdown-menu{:role => "menu"}
                  %li= link_to 'My profile',  user_user_path(current_human)
                  %li= link_to 'Edit profile', edit_user_user_path(current_human)
                  %li= link_to 'Log out', destroy_user_session_path, method: :delete
            - else
              %li= link_to 'Log In User', new_user_session_path
              %li= link_to 'Log In Admin', new_admin_session_path
              %li= link_to 'Log In Guest', new_guest_session_path
    .container-fluid
      = render 'layouts/messages'
      = yield
      = rjv_tags
