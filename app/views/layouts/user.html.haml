!!!
%html
  %head
    %title Myclass103
    = stylesheet_link_tag    'application', media: 'all'
    = javascript_include_tag 'application'
    = csrf_meta_tags
    <meta name= "viewport" content="width=device-width", initial-scale=1.0">
  %body
    %nav.navbar.navbar-default.navbar-fixed-top{:role => "navigation"}
      .container-fluid
        /
          Brand and toggle get grouped for better mobile display
        .navbar-header
          %button.navbar-toggle.collapsed{"data-target" => "#bs-example-navbar-collapse-1", "data-toggle" => "collapse", :type => "button"}
            %span.sr-only Toggle navigation
            %span.icon-bar
            %span.icon-bar
            %span.icon-bar
          %a.navbar-brand.glyphicon.glyphicon-home{:href => "/"}
        /
          Collect the nav links, forms, and other content for toggling
        #bs-example-navbar-collapse-1.collapse.navbar-collapse.text-center
          %ul.nav.navbar-nav
            %li= link_to '#Myclass103', user_root_path, class: 'navbar-brand'
            %li= link_to '#Dashboard',  user_root_path
            %li= link_to '#My profile',  user_path(current_human)
            %li= link_to '#Event groups',  user_event_groups_path
            %li= link_to 'PoS (not)',  root_path
            %li= link_to 'Payments (not)',  root_path
            %li= link_to 'root',  root_path
            %li= link_to 'Users',  users_path
            %li= link_to 'Clients',  guests_path
          %ul.nav.navbar-nav.pull-right
            - if user_signed_in?
              %li.dropdown
                %a.dropdown-toggle{"data-toggle" => "dropdown", :href => "#"}
                  = current_user.username
                  %span.caret
                %ul.dropdown-menu{:role => "menu"}
                  %li= link_to 'My profile',  user_path(current_human)
                  %li= link_to 'Edit profile', edit_user_path(current_human)
                  %li= link_to 'Log out', destroy_user_session_path, method: :delete
            - elsif admin_signed_in?
              %li.dropdown
                %a.dropdown-toggle{"data-toggle" => "dropdown", :href => "#"}
                  = current_admin.email
                  %span.caret
                %ul.dropdown-menu{:role => "menu"}
                  %li= link_to 'Edit Profile', edit_admin_registration_path
                  %li= link_to 'Log out', destroy_admin_session_path, method: :delete
            - elsif guest_signed_in?
              %li.dropdown
                %a.dropdown-toggle{"data-toggle" => "dropdown", :href => "#"}
                  = current_guest.username
                  %span.caret
                %ul.dropdown-menu{:role => "menu"}
                  %li= link_to 'My profile',  guest_path(current_human)
                  %li= link_to 'Edit Profile', edit_guest_registration_path
                  %li= link_to 'Log out', destroy_guest_session_path, method: :delete
            - else
              %li= link_to 'Log In User', new_user_session_path
              %li= link_to 'Log In Admin', new_admin_session_path
              %li= link_to 'Log In Guest', new_guest_session_path
    .container
      = yield
      = rjv_tags
      = debug(params) if Rails.env.development?