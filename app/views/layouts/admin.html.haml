!!!
%html
  %head
    %title Myclass103
    = stylesheet_link_tag    'application', media: 'all'
    = javascript_include_tag 'application'
    = csrf_meta_tags
  %body#body1
    %nav.navbar.navbar-default#navbartransparent
      .container-fluid
        .navbar-header
          %button.navbar-toggle.collapsed{"aria-controls" => "navbar", "aria-expanded" => "false", "data-target" => "#navbar", "data-toggle" => "collapse", :type => "button"}
            %span.sr-only Toggle navigation
            %span.icon-bar
            %span.icon-bar
            %span.icon-bar
          %a.navbar-brand.glyphicon.glyphicon-home{:href => "/"}
        #navbar.navbar-collapse.collapse
          %ul.nav.navbar-nav
            %li= link_to 'Myclass103', admin_root_path, class: 'navbar-brand'
            %li#uno= link_to '#Dashboard',  admin_root_path
            %li#uno= link_to 'Stats',  admin_dashboard_stats_path
            %li#uno= link_to 'Event groups',  admin_event_groups_path
            %li#uno= link_to 'Events TD',  admin_events_path
            %li#uno= link_to 'Attendances TD', admin_attendances_path
            %li#uno= link_to 'Cash collection', admin_cash_collections_path
            %li#uno= link_to 'Expences', admin_expences_path
            %li#uno= link_to '#root',  root_path
            %li#uno= link_to '#Users',  users_path
            %li#uno= link_to 'Clients',  admin_guests_path
          %ul.nav.navbar-nav.pull-right
            - if admin_signed_in?
              %li.dropdown
                %a.dropdown-toggle#whitelink{"data-toggle" => "dropdown", :href => "#"}
                  = current_admin.username
                  %span.caret
                %ul.dropdown-menu{:role => "menu"}
                  %li= link_to 'Profile', edit_admin_registration_path
                  %li= link_to 'Log out', destroy_admin_session_path, method: :delete
            - else
              %li= link_to 'Log In User', new_user_session_path
              %li= link_to 'Log In Admin', new_admin_session_path
              %li= link_to 'Log In Guest', new_guest_session_path
          %ul.nav.navbar-nav.pull-right
            %li.dropdown
              %a.dropdown-toggle#whitelink{"data-toggle" => "dropdown", :href => "#"}
                Settings
                %span.caret
              %ul.dropdown-menu{:role => "menu"}
                %li= link_to 'Companies', admin_companies_path
                %li= link_to 'Offices', admin_offices_path
                %li= link_to 'Rooms', admin_rooms_path
                %li= link_to 'Service Types', admin_service_names_path
                %li= link_to 'Services', admin_services_path
                %li= link_to 'Attendance Rates', admin_attendance_rates_path
                %li= link_to 'Attr1 Age', admin_attr1s_path
                %li= link_to 'Attr2 Lvl', admin_attr2s_path
                %li= link_to 'Attr3 Book', admin_attr3s_path
    .container-fluid
      = render 'layouts/messages'
      = yield
      = rjv_tags